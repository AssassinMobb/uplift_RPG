            <string>/// player control logic
{
    if (keyboard_check(vk_right) &amp;&amp; place_free(x+1,y)){
         motion_add(0, 1);
    }
     if (keyboard_check(vk_left) &amp;&amp; place_free(x-1,y)) {
         motion_add(180, 1);
    }
     if (keyboard_check(vk_up) &amp;&amp; place_free(x,y-1)) {
         motion_add(90, 1);
    }
     if (keyboard_check(vk_down) &amp;&amp; place_free(x,y+1)) {
        motion_add(270, 1);
    }
    
    if (abs(speed)&gt;0) friction = surface_friction;
    else friction = 0;
    
    if (abs(speed)&gt;5) speed = 5;
    /// arrow fire
    if (keyboard_check(vk_control) &amp;&amp; canShoot){
        instance_create(x-16,y-16,object_arrow); 
        canShoot = false;
        alarm[0] = 45; //set alarm 15 steps
        
    }
    /// sword slash
    if (keyboard_check(vk_space) &amp;&amp; canSlash) {
        instance_create(x,y, object_slash);
        canSlash = false;
        alarm[1] = 15;
    }
    
}


</string>
